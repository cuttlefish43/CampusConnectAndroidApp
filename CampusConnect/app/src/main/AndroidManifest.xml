<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dbmsproject.campusconnect">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampusConnect">
        <activity android:name=".UploadMaterial"
            android:label="Upload"
            android:theme="@android:style/Theme.Material.Light.Dialog"/>
        <activity
            android:name=".StudentsendMessage"
            android:label="Type Message"
            android:theme="@android:style/Theme.Material.Light.Dialog" />
        <activity android:name=".FacultyDashboard" />
        <activity android:name=".StudentDashBoard" />
        <activity
            android:name=".UserCourseReg"
            android:label="Register Course"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />
        <activity
            android:name=".addNewCourse"
            android:label="Add New Course"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".AdminDashboard" />
        <activity
            android:name=".AdminLogin"
            android:label="Admin Login"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".CourseDashboard" />
        <activity android:name=".Signin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>